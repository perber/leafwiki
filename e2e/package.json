{
  "name": "leafwiki-e2e",
  "private": true,
  "version": "0.0.0",
  "description": "End-to-end tests for LeafWiki",
  "scripts": {
    "lint": "eslint '{tests,pages}/**/*.ts' --max-warnings=0",
    "lint:fix": "eslint '{tests,pages}/**/*.ts' --fix",
    "format": "prettier \"{tests,pages}/**/*.{ts,tsx,js,jsx}\" --write",
    "format:check": "prettier \"{tests,pages}/**/*.{ts,tsx,js,jsx}\" --check",
    "install:browsers": "playwright install --with-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
