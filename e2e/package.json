{
  "name": "leafwiki-e2e",
  "private": true,
  "version": "0.0.0",
  "description": "End-to-end tests for LeafWiki",
  "scripts": {
    "lint": "eslint 'tests/**/*.ts' --max-warnings=0",
    "lint:fix": "eslint 'tests/**/*.ts' --fix",
    "format": "prettier \"tests/**/*.{ts,tsx,js,jsx}\" --write",
    "format:check": "prettier \"tests/**/*.{ts,tsx,js,jsx}\" --check",
    "install:browsers": "playwright install --with-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
